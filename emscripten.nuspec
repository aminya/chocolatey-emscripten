<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>emscripten</id>
    <version>3.1.7</version>
    <packageSourceUrl>https://github.com/aminya/chocolatey-emscripten</packageSourceUrl>
